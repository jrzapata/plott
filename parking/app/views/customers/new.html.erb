<h1>Form Page for Customer</h1>

<%= form_for @customer do |form| %>
  First Name: <%= form.text_field :first_name %></br>
  Last Name: <%= form.text_field :last_name %></br>
  Make: <%= form.text_field :make %></br>
  Model: <%= form.text_field :model %></br>
  <%= form.hidden_field :id, :value => current_user.id %>
  <%= form.submit %>
<% end %>
